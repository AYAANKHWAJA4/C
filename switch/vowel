// Online C compiler to run C program online
#include <stdio.h>

int main()
{
  char ch;
  printf("enter character ",ch);
  scanf("%c",&ch);
  switch (ch)
  {
  case 'a':
  case 'e':
  case 'i':
  case 'o':
  case 'u':
  case 'A':
  case 'E':
  case 'I':
  case 'O':
  case 'U':
  printf("entered char is vowel");
  break ;
  default: printf("entered char is not vowel");
  }
    
}

