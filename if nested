
// numer is 9 , 0 and zero

#include <stdio.h>
main()
{
 int num ;
 printf("enter a number ");
 scanf("%d",&num);
 
     if (num != 0)
     {
         
         if (num >0 )
         {
             printf("number is positive");
         }
         else
         {
             printf("number is negative");
         }
         
     }
    
     else 
     {
         printf("number is zero");
     }
}
